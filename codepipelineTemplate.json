{
    "pipeline": {
        "name": "Serverless-Test",
        "roleArn": "arn:aws:iam::848209496828:role/AWS-CodePipeline-Service",
        "artifactStore": {
            "type": "S3",
            "location": "codepipeline-us-east-2-274526041743"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "ThirdParty",
                            "provider": "GitHub",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "Branch": "master",
                            "OAuthToken": "****",
                            "Owner": "rajithII",
                            "PollForSourceChanges": "false",
                            "Repo": "sam-test2"
                        },
                        "outputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "CodeBuild",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "Test-Serverless"
                        },
                        "outputArtifacts": [
                            {
                                "name": "MyAppBuild"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ]
                    }
                ]
            },
            {
                "name": "Staging",
                "actions": [
                    {
                        "name": "MyBetaStack",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "CloudFormation",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ActionMode": "CHANGE_SET_REPLACE",
                            "Capabilities": "CAPABILITY_IAM",
                            "ChangeSetName": "MyChangeSet",
                            "RoleArn": "arn:aws:iam::848209496828:role/cloudformation-lambda-execution-role",
                            "StackName": "MyBetaStack",
                            "TemplatePath": "MyAppBuild::outputTemplate.yaml"
                        },
                        "outputArtifacts": [
                            {
                                "name": "App"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "MyAppBuild"
                            }
                        ]
                    },
                    {
                        "name": "ExecuteStack",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "CloudFormation",
                            "version": "1"
                        },
                        "runOrder": 2,
                        "configuration": {
                            "ActionMode": "CHANGE_SET_EXECUTE",
                            "ChangeSetName": "MyChangeSet",
                            "StackName": "MyBetaStack"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "App"
                            }
                        ]
                    }
                ]
            },
            {
                "name": "ManualApproval",
                "actions": [
                    {
                        "name": "Approval",
                        "actionTypeId": {
                            "category": "Approval",
                            "owner": "AWS",
                            "provider": "Manual",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "NotificationArn": "arn:aws:sns:us-east-2:848209496828:Production-Build-Approval"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "ProductionBuild",
                "actions": [
                    {
                        "name": "BuildProduction",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "ProductionBuildGreetingLambda"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": [
                            {
                                "name": "MyApp"
                            }
                        ]
                    }
                ]
            }
        ],
        "version": 1
    }
}
